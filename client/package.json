{
  "name": "next-apollo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "next dev",
    "lint": "next lint",
    "download-schema": "apollo service:download --endpoint=http://localhost:4000/graphql graphql-schema.json",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql --addTypename types"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.4.11",
    "@types/graphql": "^14.5.0",
    "apollo": "^2.33.6",
    "graphql": "15.5.3",
    "next": "11.1.2",
    "next-apollo": "5.0.6",
    "react": "17.0.2",
    "react-apollo": "3.1.5",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.21",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "16.x"
  }
}
